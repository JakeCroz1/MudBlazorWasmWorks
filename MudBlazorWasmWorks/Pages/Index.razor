                    </MudItem>
                </MudGrid>

            </MudPaper>
        </MudItem>
        <MudItem xs="12" sm="6" md="6" lg="6" Class="">
            <MudPaper Elevation="0" Class="pa-4 d-flex align-center justify-center mud-width-full mud-theme-primary " Style="height: 100%; ">
                <MudImage Fluid="true" Src="Media/Images/jakecrosbyclipped.png" Alt="Owner" Elevation="25" Class="rounded-lg" Style=" background-image: repeating-linear-gradient(36deg, rgba(255,255,255, 0.1), rgba(255,255,255, 0.1) 1px, transparent 0px, transparent 2px);" />
            </MudPaper>
        </MudItem>
