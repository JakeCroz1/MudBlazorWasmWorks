                        <MudChip Icon="@Icons.Custom.Brands.Google" Color="Color.Tertiary">oogle Business</MudChip>
                        <MudChip Icon="@Icons.Custom.Brands.MicrosoftAzure" Color="Color.Tertiary">Azure Cloud</MudChip>
                        <MudChip Icon="@Icons.Custom.Brands.Microsoft" Color="Color.Tertiary">SharePoint</MudChip>
                        <MudChip Icon="@Icons.Material.Filled.Code" Color="Color.Tertiary">C#</MudChip>
                        <MudChip Icon="@Icons.Material.Filled.Api" Color="Color.Tertiary">API</MudChip>
